{
  "Ripple": {
    "onClear": { "type": "(key: Key) => void", "required": true },
    "ripples": { "type": "RippleOptions[]", "required": true },
    "as": {
      "type": "keyof ReactDOM",
      "description": "The HTML element to render.",
      "required": false
    },
    "disabled": {
      "type": "boolean",
      "defaultValue": false,
      "description": "If `true`, disable ripple effects when pressing a element.",
      "required": false
    },
    "isDisabled": {
      "type": "boolean",
      "defaultValue": false,
      "deprecated": "Use `disabled` instead.",
      "description": "If `true`, disable ripple effects when pressing a element.",
      "required": false
    }
  },
  "UseRipple": {
    "disabled": { "type": "boolean", "required": false },
    "isDisabled": {
      "type": "boolean",
      "deprecated": "Use `disabled` instead.",
      "required": false
    },
    "onPointerDown": { "type": "PointerEventHandler<T>", "required": false }
  }
}
